<div class="row">
  <div class="col-xs-12">
    <%= link_to image_tag(@gif.url, class: "img-responsive centered"), @gif.url, id: "gif", data: {next: gif_path(@next_gif), prev: gif_path(@prev_gif)} %>
  </div>
</div>

<br/>
<p class="text-center text-muted"><i class="icon-tags"></i> <%= tag_links(@gif) %></p>
