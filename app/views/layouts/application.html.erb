  <%= csrf_meta_tags %>
</head>
<body>

  <div class="container">
    <div class="header">
      <ul class="nav nav-pills pull-right">
      </ul>
      <%= link_to image_tag("logo.png", alt: "With Great Gifs Comes Great Responsibility", class: "img-responsive"), root_url %>
    </div>

    <%= yield %>

    <div class="footer text-center">
      <p>Built with <i class="icon-heart"></i> by <%= link_to "Chris Oliver", "http://excid3.com" %></p>
    </div>
  </div> <!-- /container -->

